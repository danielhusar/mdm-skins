    "start:prod": "NODE_ENV=production node index.js",
    "build": "NODE_ENV=production webpack --config config/production/webpack.js",
    "test": "npm run lint && npm run test:shared && npm run test:server",
    "test:shared": "NODE_ENV=test ava shared/**/*.test.{js,jsx}",
    "test:server": "NODE_ENV=test ava server/**/*.test.{js,jsx} --serial",
    "test:all": "npm run lint && npm run test:server && npm run test:shared",
    "clean": "rimraf static/assets",
    "lint": "eslint --ext .js --ext .jsx client helpers server shared",
    "fixup": "npm run lint -- --fix",
    "console:dev": "NODE_ENV=development node console.js",
    "console:prod": "NODE_ENV=production node console.js"
  },
